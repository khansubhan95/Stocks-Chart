<!DOCTYPE html>
<html>
<head>
	<title>Stocks</title>

	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

	<link rel="stylesheet" type="text/css" href="/css/custom.css">

	<script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
	
	<script src="https://code.highcharts.com/stock/highstock.js"></script>
	
	<script src="https://code.highcharts.com/stock/modules/exporting.js"></script>

	<script src="https://code.highcharts.com/modules/no-data-to-display.js"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.7.3/socket.io.min.js"></script>

	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

	<script type="text/javascript" src="/js/stocks.js"></script>
</head>
<body>

	<div id="container" style="height: 400px; min-width: 310px"></div>

	<br>

	<form method="POST" action="/" class="form-inline">
		<input id="input-symbol" type="text" class="form-control" name="symbol" required="true" placeholder="enter company symbol">

		<button type="submit" class="btn btn-default">Submit</button>
	</form>

	<br>

	<div class="row">
	<% data.forEach(function(obj){ %>
		<div class="col-sm-6 col-md-4">
			<div class="thumbnail <%= obj.symbol %>">
				<div class="caption">
					<h3><%= obj.symbol %></h3>
					<p><%= obj.name %></p>
					<p>
			        	<a href="#" id="<%= obj.symbol %>" class="btn btn-danger remove" role="button">Remove</a>
					</p>
				</div>
			</div>
		</div>	
	<% }) %>
	<div id="add"></div>
	</div>

</body>
</html>